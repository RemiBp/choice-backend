{
  "name": "express-ts-boilerplate",
  "version": "1.0.0",
  "description": "Express + TS boilerplate",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon --watch src -e ts,tsx --exec ts-node --files src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:sync": "npx typeorm-ts-node-commonjs schema:sync -d ./src/data-source.ts",
    "typeorm:cache": "npx typeorm-ts-node-commonjs cache:clear",
    "schema:drop": "npx typeorm-ts-node-commonjs -d ./src/data-source.ts",
    "migration:create": "typeorm migration:create ./src/migrations/",
    "migration:generate": "npm run typeorm migration:generate -- -d src/data-source.ts ./src/migrations/updateProducerOfferTable",
    "migration:show": "npx typeorm-ts-node-commonjs migration:show -d ./src/data-source.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d  ./src/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/data-source.ts"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.499.0",
    "@aws-sdk/client-ses": "^3.499.0",
    "@aws-sdk/cloudfront-signer": "^3.535.0",
    "@aws-sdk/s3-request-presigner": "^3.503.0",
    "axios": "^1.6.7",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "firebase-admin": "^13.4.0",
    "geojson": "^0.5.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "luxon": "^3.6.1",
    "ms": "^2.1.3",
    "multer": "^2.0.1",
    "nodemon": "^3.0.3",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "zod": "^3.22.4",
    "zod-validation-error": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/geojson": "^7946.0.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jwk-to-pem": "^2.0.3",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}